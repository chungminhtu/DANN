@model string

    @Html.DevExpress().ComboBox(settings =>
{
    settings.Name = Model+"";
    settings.Properties.IncrementalFilteringMode = IncrementalFilteringMode.Contains;
    settings.Properties.DropDownStyle = DropDownStyle.DropDownList;
    settings.CallbackRouteValues = new { Controller = "DMCode", Action = "LoadComboChucVu" };
    settings.Properties.CallbackPageSize = 50;
    settings.Properties.ValueField = "CodeName";
    settings.Properties.TextField = "CodeName";
    settings.Properties.ValueType = typeof(string);
}).BindList(ViewData["ComboListChucVu"]).GetHtml()